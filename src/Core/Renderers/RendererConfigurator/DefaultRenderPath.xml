<?xml version="1.0" encoding="UTF-8"?>
<config>
  <textures>
    <renderer-fit name="depth" layout="RGB" format="UBYTE"/>
    <renderer-fit name="g-normal" layout="RGBA" format="UBYTE"/>
    <renderer-fit name="l-diffuse" layout="RGB" format="UBYTE"/>
    <renderer-fit name="l-specular" layout="RGB" format="UBYTE"/>
    <renderer-fit name="color" layout="RGBA" format="UBYTE"/>
    <renderer-fit name="hitarea" layout="RGB" format="UBYTE"/>
  </textures>
  <path>
    <stage name="jthree.hitarea" b:OUT="hitarea"/>
    <stage name="jthree.basic.gbuffer" b:PRIMARY="g-normal" b:DEPTH="depth"/>
    <stage name="jthree.basic.light" b:PRIMARY="g-normal" b:DEPTH="depth" b:DLIGHT="l-diffuse" b:SLIGHT="l-specular"/>
    <stage name="jthree.basic.forward" b:DLIGHT="l-diffuse" b:SLIGHT="l-specular" b:OUT="color"/>
    <stage name="jthree.basic.fxaa" b:INPUT="color" b:OUTPUT="default"/>
  </path>
</config>
